{
  "manifest_version": 3,
  "name": "TubeTutor",
  "version": "1.0",
  "description": "A Chrome Extension to support learning with video suggestions.",
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "scripting",
    "contextMenus"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "64": "images/logo.png",
    "128": "images/logo.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["listeningScript.js"],
      "matches": ["<all_urls>"]
    }
  ]
  "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "js": ["content_script.js"]
    }
  ]
  "background": {
    "service_worker": "receiveKeywords.js",
    "type": "module"
  },
}
